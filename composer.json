{
    "name": "wyrihaximus/phpstan-react",
    "description": "\ud83d\udc4e ReactPHP extension for PHPStan",
    "license": "MIT",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "nikic/php-parser": "^4.18",
        "phpstan/phpdoc-parser": "^1.24",
        "wyrihaximus/makefiles": "^0.2.0",
        "wyrihaximus/test-utilities": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\PHPStan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\React\\PHPStan\\Utils\\": "utils/",
            "WyriHaximus\\Tests\\React\\PHPStan\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "icanhazstring/composer-unused": true,
            "infection/extension-installer": true,
            "wyrihaximus/makefiles": true
        },
        "platform": {
            "php": "8.2.13"
        },
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
